{
  "name": "content-services-ui",
  "version": "0.89.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-local": "ng serve --configuration=local --disable-host-check",
    "build": "ng build",
    "test": "ng test --watch=false --sourceMap=false",
    "test-cover": "rm -rf ./coverage && ng test --no-watch --code-coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "ng lint content-services-ui --type-check",
    "e2e": "ng e2e",
    "e2e-local": "yarn mock-services && ng e2e --protractor-config=protractor.conf.local.js --configuration=travis ; testStatus=$? ; yarn stop-mock-services ; exit $testStatus",
    "validate": "npm-run-all --parallel test lint",
    "prepush": "npm run validate",
    "precommit": "lint-staged",
    "pretty": "prettier --parser typescript --single-quote --print-width=120 --write",
    "mock-services": "docker container inspect content-services-api-e2e-mock || (docker build -q -t content-services-api-e2e-mock ./e2e/mocks && docker run --name content-services-api-e2e-mock --rm -d -p 12345:80 -v ${PWD}/e2e/mocks:/usr/share/nginx/html/ content-services-api-e2e-mock)",
    "stop-mock-services": "docker stop content-services-api-e2e-mock"
  },
  "lint-staged": {
    "*.ts": [
      "npm run pretty",
      "npm run lint",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.10",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/flex-layout": "^6.0.0-beta.18",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/platform-server": "^6.1.10",
    "@angular/router": "^6.1.10",
    "@types/lodash": "^4.14.116",
    "@types/pdfjs-dist": "^0.1.2",
    "angulartics2": "^6.3.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.7",
    "coveralls": "^3.0.2",
    "directory-encoder": "0.9.2",
    "jasmine": "^3.0.0",
    "lint-staged": "^7.3.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "ng2-pdf-viewer": "5.1.2",
    "ngx-daterangepicker-material": "^1.2.3",
    "pdfjs-dist": "^2.0.550",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.0.0-rc.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.4",
    "@angular/cli": "^6.2.5",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/language-service": "^6.1.9",
    "@types/jasmine": "~2.8.9",
    "@types/jasminewd2": "~2.0.5",
    "@types/node": "~10.9.4",
    "codelyzer": "^4.5.0",
    "husky": "^0.14.3",
    "jasmine-core": "~2.99.0",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "karma-parallel": "^0.3.0",
    "karma-spec-reporter": "^0.0.32",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "protractor": "^5.4.1",
    "ts-node": "^7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.1.4"
  }
}
