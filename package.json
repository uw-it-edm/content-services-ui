{
  "name": "content-services-ui",
  "version": "0.89.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-local": "ng serve --configuration=local --disable-host-check",
    "start-localmock": "ng serve --configuration=localmock --disable-host-check",
    "build": "ng build",
    "test": "ng test --watch=false --sourceMap=false",
    "test-cover": "rm -rf ./coverage && ng test --no-watch --code-coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "ng lint content-services-ui --type-check",
    "e2e": "ng e2e",
    "e2e-local": "$npm_execpath mock-services && ng e2e --protractor-config=protractor.conf.local.js --configuration=travis ; testStatus=$? ; yarn stop-mock-services ; exit $testStatus",
    "validate": "npm-run-all --parallel test lint",
    "pretty": "prettier --parser typescript --single-quote --print-width=120 --write",
    "mock-services": "$npm_execpath start-mock-services",
    "start-mock-services": "docker container inspect content-services-api-e2e-mock || (docker build -q -t content-services-api-e2e-mock ./e2e/mocks && docker run --name content-services-api-e2e-mock --rm -d -p 12345:80 -v ${PWD}/e2e/mocks:/usr/share/nginx/html/ content-services-api-e2e-mock)",
    "stop-mock-services": "docker stop content-services-api-e2e-mock"
  },
  "lint-staged": {
    "linters": {
      "*.ts": [
        "npm run pretty",
        "git add"
      ]
    },
    "relative": true
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.7",
    "@angular/cdk": "^7.3.3",
    "@angular/common": "^7.2.7",
    "@angular/compiler": "^7.2.7",
    "@angular/core": "^7.2.7",
    "@angular/flex-layout": "^7.0.0-beta.24",
    "@angular/forms": "^7.2.7",
    "@angular/http": "^7.2.7",
    "@angular/material": "^7.3.3",
    "@angular/platform-browser": "^7.2.7",
    "@angular/platform-browser-dynamic": "^7.2.7",
    "@angular/platform-server": "^7.2.7",
    "@angular/router": "^7.2.7",
    "@types/lodash": "^4.14.134",
    "@types/pdfjs-dist": "^2.0.0",
    "angulartics2": "^6.3.1",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.6.5",
    "coveralls": "^3.0.4",
    "directory-encoder": "0.9.2",
    "lint-staged": "^8.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "ng2-pdf-viewer": "5.3.0",
    "ngx-daterangepicker-material": "^2.1.3",
    "pdfjs-dist": "^2.0.943",
    "rxjs": "^6.5.2",
    "tslib": "^1.9.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.9",
    "@angular/cli": "^7.3.9",
    "@angular/compiler-cli": "^7.2.7",
    "@angular/language-service": "^8.0.0",
    "@types/jasmine": "~3.3.11",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~12.0.5",
    "axe-webdriverjs": "^2.2.0",
    "codelyzer": "^5.0.1",
    "husky": "^2.4.0",
    "jasmine-core": "^3.4.0",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-parallel": "^0.3.1",
    "karma-spec-reporter": "^0.0.32",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.1",
    "protractor": "^5.4.2",
    "rxjs-tslint-rules": "^4.23.2",
    "ts-node": "^8.2.0",
    "tslint": "~5.16.0",
    "typescript": "~3.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run validate"
    }
  }
}
