{
  "name": "content-services-ui",
  "version": "0.89.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "replace-vars": "node --experimental-modules ./src/environments/replace-vars.mjs",
    "start-local": "ng serve --configuration=local --disable-host-check",
    "start-travis": "ng serve --configuration=travis --disable-host-check",
    "start-localproxy": "yarn replace-vars && ng serve --configuration=localproxy --disable-host-check",
    "start-localmock": "ng serve --configuration=localmock --disable-host-check",
    "start-localmock-ie11": "ng serve --configuration=localmock-ie11 --disable-host-check",
    "build": "ng build",
    "test": "ng test --watch=false --sourceMap=false",
    "test-tdd": "ng test --watch=true --sourceMap=true --browsers ChromeDebugging --karmaConfig karma.tdd.config.js",
    "test-cover": "rm -rf ./coverage && ng test --no-watch --code-coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "ng lint content-services-ui --type-check",
    "e2e": "ng e2e",
    "e2e-local": "$npm_execpath mock-services && node --max_old_space_size=4096 'node_modules/@angular/cli/bin/ng' e2e --protractor-config=protractor.conf.local.js --configuration=travis ; testStatus=$? ; yarn stop-mock-services ; exit $testStatus",
    "validate": "npm-run-all --parallel test lint",
    "pretty": "prettier --parser typescript --single-quote --print-width=140 --write",
    "mock-services": "$npm_execpath start-mock-services",
    "start-mock-services": "docker container inspect content-services-api-e2e-mock || (docker build -q -t content-services-api-e2e-mock ./e2e/mocks && docker run --name content-services-api-e2e-mock --rm -d -p 12345:80 -v ${PWD}/e2e/mocks:/usr/share/nginx/html/ content-services-api-e2e-mock)",
    "stop-mock-services": "docker stop content-services-api-e2e-mock"
  },
  "lint-staged": {
    "*.ts": [
      "yarn run pretty",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.14",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "^10.0.14",
    "@angular/compiler": "^10.0.14",
    "@angular/core": "^10.0.14",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "^10.0.14",
    "@angular/material": "^10.1.3",
    "@angular/platform-browser": "^10.0.14",
    "@angular/platform-browser-dynamic": "^10.0.14",
    "@angular/platform-server": "^10.0.14",
    "@angular/router": "^10.0.14",
    "angulartics2": "^6.3.1",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.6.11",
    "directory-encoder": "0.9.2",
    "lodash": "^4.17.20",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.28",
    "ng2-pdf-viewer": "6.2.0",
    "ngx-daterangepicker-material": "^2.3.0",
    "pdfjs-dist": "2.3.200",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.3",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.11.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.8",
    "@angular/cli": "^10.0.8",
    "@angular/compiler-cli": "^10.0.14",
    "@angular/language-service": "^10.0.14",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "~2.0.8",
    "@types/lodash": "^4.14.165",
    "@types/node": "^12.19.9",
    "@types/pdfjs-dist": "2.1.4",
    "axe-webdriverjs": "^2.2.0",
    "codelyzer": "^6.0.1",
    "coveralls": "^3.1.0",
    "husky": "^4.3.5",
    "jasmine-core": "~3.6.0",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-parallel": "^0.3.1",
    "karma-spec-reporter": "^0.0.32",
    "lint-staged": "^10.5.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "protractor": "^7.0.0",
    "rxjs-tslint-rules": "4.34.7",
    "ts-node": "^9.1.1",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run validate"
    }
  }
}